<% if(typeof errors!= 'undefined') { %>
<%    errors.forEach(function(error){ %>
<p style="color: red; margin-left: 10px;"> <%= error.msg %></p>
<%    })        %>
<% } %>
<% if(success_msg!= '') { %>
<p style="color: green; margin-left: 10px;"><%=success_msg %> </p>
<% } %>
<% if(error_msg != '') { %>
<p style="color: red; margin-left: 10px;"><%= error_msg %></p>
<% } %>
<% if(error != '') { %>
<p style="color: red; margin-left: 10px;"><%= error %></p>
<% } %>